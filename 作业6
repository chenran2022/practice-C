int main()
{
	char arr[3][3] = { 0 };
	int i = 0;
	int j = 0;
	for (i = 0; i < 3; i++)
	{
		for (j = 0; j < 3; j++)
		{
			scanf(" %c", &arr[i][j]);
		}
	}
	char flag = 0;
	for (i = 0; i < 3; i++)
	{
		if (arr[i][0] == arr[i][1] == arr[i][2] != 'O')
		{
			flag = arr[i][0];
			break;
		}
		if (arr[0][i] == arr[1][i] == arr[2][i] != 'O')
		{
			flag = arr[0][i];
			break;
		}
		
	}
	if (arr[0][0] == arr[1][1] == arr[2][2] != 'O')
	{
		flag = arr[1][1];	
	}
	if (arr[2][0] == arr[1][1] == arr[0][2] != 'O')
	{
		flag = arr[1][1];
	}
	
	if (flag == 'R')
		printf("R win!\n");
	else if (flag == 'B')
		printf("B win!\n");
	else
		printf("NO win!\n");
	
	return 0;
}
